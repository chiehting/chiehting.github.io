 &lt;script&gt;
 $("document").ready(function(){
 $("#head img").attr("src","https://storage.googleapis.com/chiehting.com/avatar.png");
 $.each($(".star"), function(index, value) {
 var star = $(value).html();
 var tmp = '';
 for(i = 0; i &lt; 5; i++) {
 if((star-i)&gt;=1){
 tmp += '&lt;i class="fa fa-star" aria-hidden="true"&gt;&lt;/i&gt;';
 }else if((star-i)&gt;0 &amp;&amp; (star-i)&lt;1){
 tmp += '&lt;i class="fa fa-star-half-o" aria-hidden="true"&gt;&lt;/i&gt;';
 }else{
 tmp += '&lt;i class="fa fa-star-o" aria-hidden="true"&gt;&lt;/i&gt;';
 }
 $(value).html(tmp);
 }
 });
 });
 &lt;/script&gt;

